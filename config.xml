<?xml version='1.0' encoding='utf-8'?>
<widget id="com.tutsplus.agentassist" version="0.0.1" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>agentAssist</name>
    <description>
        A sample Apache Cordova application that responds to the deviceready event.
    </description>
    <author email="dev@cordova.apache.org" href="http://cordova.io">
        Apache Cordova Team
    </author>
    
    <preference name="SplashScreen" value="screen"/>
    <preference name="SplashScreenDelay" value="3000"/>
    <platform name="android">
        <icon src="www/icons/icon.png"/>
        
        <splash src="www/screen/splash-hdpi.png" density="land-hdpi"/>
        <splash src="www/screen/splash-ldpi.png" density="land-ldpi"/>
        <splash src="www/screen/splash-mdpi.png" density="land-mdpi"/>
        <splash src="www/screen/splash-xhdpi.png" density="land-xhdpi"/>
        
        <splash src="www/screen/splash-hdpi.png" density="port-hdpi"/>
        <splash src="www/screen/splash-ldpi.png" density="port-ldpi"/>
        <splash src="www/screen/splash-mdpi.png" density="port-mdpi"/>
        <splash src="www/screen/splash-xhdpi.png" density="port-xhdpi"/>
    </platform>
    
    <content src="index.html" />
    <plugin name="cordova-plugin-whitelist" version="1" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <platform name="android">
        <allow-intent href="market:*" />
    </platform>
    <platform name="ios">
        <allow-intent href="itms:*" />
        <allow-intent href="itms-apps:*" />
    </platform>
</widget>
